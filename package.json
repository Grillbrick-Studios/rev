		"lint": "eslint 'src/**/*.{js,ts,tsx}' --fix"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jhessin/rev.git"
	},
	"keywords": [],
	"files": [
		"dist/**/*"
	],
